package GUI;

/**
 *
 * @author Tom Pepels - 25-9-2011
 */
public class ComboItem {
    private String string;
    private int id;

    public ComboItem(String string, int id) {
        this.string = string;
        this.id = id;
    }

    @Override
    public String toString() {
        return string;
    }

    /**
     * @return the id
     */
    public int getId() {
        return id;
    }
}
